////////Tabels///////////
#include <stdio.h>
int table(int,int);
intmain()
{
intn,d,i;
printf("\nEnter elements ");
scanf("%d",&n);
    table(n,i);
    return 0;
}
int table(intn,inti)
{

 for(i=1;i<=10;i++)
 {
printf("%d\n",n*i);
 }
 }

/////////////////////////////1.Pattern
#include <stdio.h>
void main()
{
inti,j,k;
    for(i=1;i<=3;i++)
    {

       k=1;
       for(j=1;j<=5;j++)
       {
           if(j>=4-i&&j<=2+i&&k)
           {
printf("*");
               k=0;
           }
           else
           {
printf(" ");
               k=1;

           }
       }
printf("\n");
}
}/////////////////Nth max value////////////////////

#include <stdio.h>
intmax(int []);
intmain()
{
inta[5],m,i;
printf("\nEnter 5 values ");
    for(i=0;i<5;i++)
scanf("%d",&a[i]);
    m=max(a);
printf("max=%d",m);
    return 0;
}
intmax(int a[])
{
inti,m=-32768;
    for(i=0;i<5;i++)
     if(a[i]>m)
     {
         m=a[i];
     }
     return m;
}
///////////////////////Substring without function///////////////////////
#include<stdio.h>
#include<string.h>
void main()
{
    char a[10],b[10];
int l1,l2,max,i;
printf("enter a string");
scanf("%s",a);
printf("enter a sub string");
scanf("%s",b);
    l1=strlen(a);
    l2=strlen(b);
    max=l1-l2;
    for(i=0;i<=max;i++)
      if(strncmp(a+i,b,l2)==0)
           break;
    if(i<=max) 
printf("found=%d",i);
    else
printf("not found");
}
////////////////////Substrings with functions////////////////////
#include<stdio.h>
#include<string.h>
void subs(char [],char []);
int main()
{
    char a[10],b[10];
printf("enter a string");
scanf("%s",a);
printf("enter a sub string");
scanf("%s",b);
    subs(a,b);
    return 0;
}
void subs(char a[],char b[])
{
int l1,l2,max,i;
    l1=strlen(a);
    l2=strlen(b);
    max=l1-l2;
    for(i=0;i<=max;i++)
      if(strncmp(a+i,b,l2)==0)
           break;
    if(i<=max) 
printf("found=%d",i);
    else
printf("not found");
}

//////////////////////2.Pattern
#include <stdio.h>
void main()
{
inti,j,k;
    for(i=1;i<=3;i++)
    {

       k=1;
       for(j=1;j<=5;j++)
       {
           if(j>=i&&j<=6-i&&k)
           {
printf("*");
               k=0;
           }
           else
           {
printf(" ");
               k=1;

           }
       }
printf("\n");
}
}
////////////////////////Reverse///////////////////
#include<stdio.h>
intrev(char []);
intmain()
{
    char str[10];
inti;
printf("enter a string");
    for(i=0;str[i]='\0';i++)
    {
scanf("%c",&str[i]);
    }
    rev(str);
    return 0;
}
intrev(char str[])
{
inti, c=0;
    for(i=0;str[i]='\0';i++)
    {
c++;
    }
      for(i=c-1;i>=0;i--) 
      {
printf("%c",str[i]);
}
}
